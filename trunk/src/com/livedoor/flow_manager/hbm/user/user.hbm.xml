<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.livedoor.flow_manager.user.beans.User" table="t_user">
<!--		<cache usage="read-write" region="com.livedoor.flow_manager.common.cacheTemplate"/>-->
		
		<id column="USER_ID"
			name="userId"
			type="java.lang.Integer">
			<generator class="identity" />
		</id>
		
		<property
			column="USER_PHOTO"
			length="30"
			name="userPhoto"
			not-null="false"
			type="string"
		 />
		<property
			column="UPDATE_USER_ID"
			length="11"
			name="updateUserId"
			not-null="false"
			type="integer"
		 />
		<property
			column="USER_MSN"
			length="30"
			name="userMsn"
			not-null="false"
			type="string"
		 />
		<property
			column="INPUT_DATETIME"
			length="19"
			name="inputDatetime"
			not-null="false"
			type="timestamp"
		 />
		<property
			column="USER_EMAIL"
			length="30"
			name="userEmail"
			not-null="false"
			type="string"
		 />
		<property
			column="DELETED_FLAG"
			length="11"
			name="deletedFlag"
			not-null="true"
			type="integer"
		 />
		<property
			column="USER_SKYPE"
			length="30"
			name="userSkype"
			not-null="false"
			type="string"
		 />
		 <!--  -->
		<property
			column="ACCOUNT_NON_EXPIRED"
			length="4"
			name="accountNonExpiredInt"
			not-null="true"
			type="java.lang.Integer"
		 />
		<property
			column="ACCOUNT_NON_LOCKED"
			length="4"
			name="accountNonLockedInt"
			not-null="true"
			type="java.lang.Integer"
		 />
		<property
			column="CREDENTIALS_NON_EXPIRED"
			length="4"
			name="credentialsNonExpiredInt"
			not-null="true"
			type="java.lang.Integer"
		 />
		 

		<property
			column="USER_DESC"
			length="500"
			name="userDesc"
			not-null="false"
			type="string"
		 />
		 
		 
<!--		<property-->
<!--			column="USER_ROLE"-->
<!--			length="100"-->
<!--			name="userRole"-->
<!--			not-null="false"-->
<!--			type="string"-->
<!--		 />-->

				<many-to-one name="role"
				column="USER_ROLE"
				lazy="false"
				class="com.livedoor.flow_manager.role.beans.Role"/>

		<property
			column="UPDATE_DATETIME"
			length="19"
			name="updateDatetime"
			not-null="false"
			type="timestamp"
		 />
		<property
			column="INPUT_USER_ID"
			length="11"
			name="inputUserId"
			not-null="true"
			type="integer"
		 />
		<property
			column="USER_NAME"
			length="30"
			name="userName"
			not-null="true"
			type="java.lang.String"
		 />
		<property
			column="USER_PASSWORD"
			length="30"
			name="userPassword"
			not-null="true"
			type="java.lang.String"
		 />
		 <property
			column="USER_DISPLAY_NAME"
			length="30"
			name="userDisplayName"
			not-null="true"
			type="java.lang.String"
		 />
	</class>
</hibernate-mapping>
